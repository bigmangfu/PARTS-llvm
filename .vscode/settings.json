{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "${workspaceFolder}/build": true,
        "${workspaceFolder}/cmake-build-debug": true
    },
    "cmake.buildDirectory": "${workspaceRoot}/build-vscode",
    "cmake.configureArgs": [
        "-DCMAKE_INSTALL_PREFIX=/home/ishkamiel/opt/llvm/llvm-rel60_debug",
        "-DLLVM_OCAML_INSTALL_PATH=/home/ishkamiel/opt/llvm/ocaml-rel60_debug",
        "-DCMAKE_BUILD_TYPE=Debug",
        "-DBUILD_SHARED_LIBS=On",
        "-DLLVM_TARGETS_TO_BUILD=AArch64",
        "-DLLVM_PARALLEL_LINK_JOBS=",
        "-DLLVM_PARALLEL_COMPILE_JOBS=",
        "-DLLVM_INSTALL_BINUTILS_SYMLINKS=On",
        "-DLLVM_INSTALL_CCTOOLS_SYMLINKS=On",
        "-DLLVM_CCACHE_BUILD=On",
        "-DLLVM_BUILD_TOOLS=On",
        "-DLLVM_BUILD_TESTS=Off",
        "-DLLVM_BUILD_EXAMPLES=Off",
        "-DLLVM_BUILD_DOCS=Off",
        "-DLLVM_INCLUDE_EXAMPLES=Off",
        "-DLLVM_ENABLE_CXX1Y=On",
        "-DLLVM_ENABLE_LTO=Off",
        "-DLLVM_ENABLE_DOXYGEN=Off",
        "-DLLVM_ENABLE_RTTI=Off",
        "-DCMAKE_C_COMPILER=/usr/lib/llvm-6.0/bin/clang",
        "-DLLDB_TEST_C_COMPILER=/usr/lib/llvm-6.0/bin/clang",
        "-DCMAKE_CXX_COMPILER=/usr/lib/llvm-6.0/bin/clang++",
        "-DLLDB_TEST_CXX_COMPILER=/usr/lib/llvm-6.0/bin/clang++",
        "-DLLVM_USE_LINKER=/usr/lib/llvm-6.0/bin/ld.lld"
    ]
}
